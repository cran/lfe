> ## extract the group fixed effects
> for(ef in c('ln','ref')) {
+   fe <- getfe(est,ef=ef)
+   ## merge back
+ 
+   ideff <- fe[paste('id',id,sep='.'),'effect']
+   firmeff <- fe[paste('firm',firm,sep='.'),'effect']
+   shoeeff <- fe[paste('shoe',shoe,sep='.'),'effect']
+ 
+   ## verify that id and firm coefficients are 1
