> # test getfe:
> Kid <- nlevels(id)
> sid <- 1:Kid
> Kfirm <- nlevels(firm)
> sfirm <- 1:Kfirm + Kid
> ef <- function(x, addnames=FALSE) {
+   icpt <- x[1] + x[Kid+1]
+   x[sid[-1]] <- x[sid[-1]] - x[1]
+   x[1] <- icpt
+   x[sfirm] <- x[sfirm] - x[Kid+1]
+   if(addnames) names(x) <- c('(Intercept)',paste('id',sid[-1],sep=''),paste('firm',1:Kid,sep=''))
+   x[c(sid,sfirm[-1])]
+ }
